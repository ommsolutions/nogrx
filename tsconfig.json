{
    "compilerOptions": {
        "declaration": true,
        "sourceMap": true,
        "module": "es2015",
        "noImplicitAny": false,
        "target": "es5",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "allowJs": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "suppressImplicitAnyIndexErrors": true,
        "emitDecoratorMetadata": true
    },
    "exclude": [
        "node_modules"
    ]
}